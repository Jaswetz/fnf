{
  "AWSTemplateFormatVersion" : "2010-09-09",
  "Resources" : {
    "teams" : {
      "Type": "AWS::DynamoDB::Table",
      "Properties": {
        "TableName": "teams",
        "AttributeDefinitions": [
          {
            "AttributeName": "name",
            "AttributeType": "S"
          }
        ],
        "KeySchema": [
          {
            "AttributeName": "name",
            "KeyType": "HASH"
          }
        ],
        "ProvisionedThroughput": {
          "ReadCapacityUnits": "1",
          "WriteCapacityUnits": "1"
        }
      }
    },
    "waitingList" : {
      "Type": "AWS::DynamoDB::Table",
      "Properties": {
        "TableName": "waitingList",
        "AttributeDefinitions": [
          {
            "AttributeName" : "showName",
            "AttributeType" : "S"
          },
          {
            "AttributeName" : "signUpDate",
            "AttributeType" : "N"
          }
        ],
        "KeySchema" : [
          {
            "AttributeName" : "showName",
            "KeyType" : "HASH"
          },
          {
            "AttributeName" : "signUpDate",
            "KeyType" : "RANGE"
          }
        ],
        "ProvisionedThroughput" : {
          "ReadCapacityUnits" : "1",
          "WriteCapacityUnits" : "1"
        }
      }
    }
  }
}